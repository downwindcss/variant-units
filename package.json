{
	"name": "@downwindcss/variant-units",
	"author": "dance2die",
	"version": "0.0.1-alpha.0",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [ "dist", "src" ],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"version:alpha": "pnpm version prerelease --preid=alpha",
		"publish:alpha": "pnpm publish --tag alpha",
		"version:beta": "pnpm version prerelease --preid=beta",
		"publish:beta": "pnpm publish --tag beta"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"module": "dist/variant-units.esm.js",
	"size-limit": [
		{
			"path": "dist/variant-units.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/variant-units.esm.js",
			"limit": "10 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^4.10.2",
		"@types/tailwindcss": "^2.0.3",
		"husky": "^6.0.0",
		"postcss": "^8.2.15",
		"size-limit": "^4.10.2",
		"tailwindcss": "^2.1.2",
		"tsdx": "^0.14.1",
		"tslib": "^2.2.0",
		"typescript": "^4.2.4"
	}
}
